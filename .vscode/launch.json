{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Compiled",
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "smartStep": false,
            "skipFiles": [
                "<node_internals>/**/*.js",
                "${workspaceRoot}/dist/test.js"
            ],
            "args": ["./dist/test.js"],
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}